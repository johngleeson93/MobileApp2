<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/landmarkFragment">

    <fragment
        android:id="@+id/landmarkFragment"
        android:name="org.wit.landmark.fragments.LandmarkFragment"
        android:label="@string/action_landmark"
        tools:layout="@layout/fragment_landmark" >
        <action
            android:id="@+id/action_landmarkFragment_to_landmarkListFragment"
            app:destination="@id/landmarkListFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/landmarkListFragment"
        android:name="org.wit.landmark.fragments.landmarkListFragment"
        android:label="@string/action_landmarkList"
        tools:layout="@layout/fragment_landmarkList" >
        <action
            android:id="@+id/action_landmarkListFragment_to_landmarkFragment"
            app:destination="@id/landmarkFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
    </fragment>
</navigation>